    "@tailwindcss/forms": "0.4.0",
    "autoprefixer": "10.4.2",
    "file-saver": "2.0.5",
    "page": "1.11.6",
    "sirv-cli": "2.0.1",
    "svelte-forms-lib": "2.0.1",
    "tailwind-hamburgers": "1.1.1",
    "tailwindcss": "3.0.15",
    "yup": "0.32.11"
