    "@rollup/plugin-typescript": "8.3.0",
    "@tsconfig/svelte": "3.0.0",
    "@types/file-saver": "2.0.5",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-svelte3": "^3.4.0",
    "prettier": "^2.5.1",
